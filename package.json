{
  "name": "@enspirit/elo",
  "version": "0.9.5",
  "description": "A small expression language that compiles to JavaScript (JS-only fork)",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "files": [
    "dist/src"
  ],
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "test": "bun run test:unit",
    "test:unit": "bun test test/unit/",
    "test:watch": "tsc --watch & bun test --watch test/unit/",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "docker:logs": "docker compose logs -f",
    "format": "prettier --write ."
  },
  "keywords": [
    "compiler",
    "expression",
    "multi-target"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/enspirit/elo.git"
  },
  "homepage": "https://elo-lang.org",
  "bugs": {
    "url": "https://github.com/enspirit/elo/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "luxon": "^3.5.0",
    "prettier": "^3.8.1"
  },
  "devDependencies": {
    "@codemirror/autocomplete": "^6.20.0",
    "@codemirror/commands": "^6.10.1",
    "@codemirror/language": "^6.12.1",
    "@codemirror/state": "^6.5.3",
    "@codemirror/view": "^6.39.7",
    "@hotwired/stimulus": "^3.2.2",
    "@lezer/highlight": "^1.2.3",
    "@types/luxon": "^3.4.0",
    "@types/node": "^20.10.0",
    "@types/pg": "^8.10.0",
    "codemirror": "^6.0.2",
    "esbuild": "^0.27.2",
    "pg": "^8.11.0",
    "typescript": "^5.3.0"
  }
}
