assert(fetch({name: 'Alice'}, .name) == 'Alice')
assert(fetch({user: {name: 'Bob'}}, .user.name) == 'Bob')
assert(fetch({items: [10, 20, 30]}, .items.0) == 10)
assert(fetch({items: [10, 20, 30]}, .items.2) == 30)
assert(fetch({list: [{id: 1}, {id: 2}]}, .list.0.id) == 1)
assert(isNull(fetch({name: 'Alice'}, .missing)))
assert(isNull(fetch({name: 'Alice'}, .name.child)))
assert(isNull(fetch(null, .x)))
assert(fetch({a: 1, b: 2}, {x: .a, y: .b}) == {x: 1, y: 2})
assert(fetch({foo: {bar: 10}}, {v: .foo.bar}) == {v: 10})
assert(fetch({items: [1, 2, 3]}, {first: .items.0, last: .items.2}) == {first: 1, last: 3})
assert(fetch({x: 12}, 'x') == 12)
assert(fetch({x: 12}, ['x']) == 12)
assert(fetch({user: {name: 'Bob'}}, ['user', 'name']) == 'Bob')