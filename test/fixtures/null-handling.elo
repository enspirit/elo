assert(isVal(42) == true)
assert(isVal('hello') == true)
assert(isVal(true) == true)
assert(orVal(42, 0) == 42)
assert(orVal('hello', 'default') == 'hello')
assert(orVal(1 + 1, 0) == 2)
assert(indexOf('hello', 'l') == 2)
assert(isVal(indexOf('hello', 'l')) == true)
assert(isVal(indexOf('hello', 'x')) == false)
assert(typeOf(indexOf('hello', 'x')) == 'NoVal')
assert(orVal(indexOf('hello', 'x'), -1) == -1)
assert(orVal(indexOf('hello', 'l'), -1) == 2)
